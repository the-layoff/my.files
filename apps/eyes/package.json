    "scripts": {
    "test": "TZ=UTC jest",
    "clean": "manypkg exec rm -rf node_modules && manypkg exec rm -rf ./apps/matrix/dist && rm -rf apps/matrix/node_modules",
    "start:matrix": "yarn webpack serve",
    "start:brain": "echo figure out how to start a python api server",
    "start": "echo figure out how to start both services simultaneously here",
    "lint:css": "./node_modules/.bin/stylelint \"src/app/**/*.css\" --config ./.config/stylelint.config.js --max-warnings 0 --fix",
    "lint:js": "./node_modules/.bin/eslint --config ./.config/eslint.config.js --ext .js,.jsx,.ts,.tsx --fix --max-warnings 0",
    "lint:css-no-fix": "./node_modules/.bin/stylelint \"src/app/**/*.css\" --config ./.config/stylelint.config.js --max-warnings 0",
    "lint:js-no-fix": "./node_modules/.bin/eslint --config ./.config/eslint.config.js --max-warnings 0",
    "format": "prettier --find-config-path ./.config/prettier.config.js --write",
    "format:check": "prettier --find-config-path ./.config/prettier.config.js --check",
    "stylelint:fix": "stylelint --fix --config ./.config/stylelint.config.js --max-warnings 0",
    "stylelint:check": "stylelint --config ./.config/stylelint.config.js --max-warnings 0",
    "eslint:fix": "eslint --fix --config ./.config/eslint.config.js --max-warnings 0",
    "eslint:check": "eslint --config ./.config/eslint.config.js --max-warnings 0",
    "check-size": "size-limit --why",
    "serve": "NODE_ENV=development webpack serve",
    "build:dev": "NODE_ENV=development webpack && yarn run build:css:dev",
    "build:prod": "NODE_ENV=production webpack",
    "build:css:dev": "tailwindcss -c ./.config/tailwindcss-config.js -i ./src/app/base/index.css -o ./public/static/css/main.css",
    "build:storybook": "yarn build-storybook -o ./src/storybook",
    "storybook": "yarn build:storybook && yarn start-storybook"
  },
    "browserslist": {
        "production": [
            ">0.25%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
